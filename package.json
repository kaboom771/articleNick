{
  "name": "articlenick",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "b": "pnpm --filter @articleNick/backend",
    "w": "pnpm --filter @articleNick/webapp",
    "dev": "pnpm -r --parallel dev",
