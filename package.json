{
  "name": "articleNick",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "b": "pnpm --filter @articleNick/backend",
    "w": "pnpm --filter @articleNick/webapp",
    "dev": "pnpm -r --parallel dev",
    "types": "concurrently 'pnpm b types' 'pnpm w types'",
    "lint": "concurrently 'pnpm b lint' 'pnpm w lint'",
    "prettify": "pnpm -r --parallel prettify",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "concurrently": "^8.2.0",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0"
  }
}
